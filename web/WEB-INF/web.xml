<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UsuarioController</servlet-name>
        <servlet-class>br.com.spout.controller.UsuarioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthenticateController</servlet-name>
        <servlet-class>br.com.spout.controller.AuthenticateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdministratorController</servlet-name>
        <servlet-class>br.com.spout.controller.AdministratorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdministratorAuthenticate</servlet-name>
        <servlet-class>br.com.spout.controller.AdministratorAuthenticate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioController</servlet-name>
        <url-pattern>/UsuarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthenticateController</servlet-name>
        <url-pattern>/AuthenticateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdministratorController</servlet-name>
        <url-pattern>/Admin</url-pattern>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdministratorAuthenticate</servlet-name>
        <url-pattern>/AdministratorAuthenticate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
